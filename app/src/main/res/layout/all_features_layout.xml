<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_30sdp">

        <workshop.lbit.qrcode.customfonts.MyTextView_Roboto_Medium
            android:id="@+id/ll_store_boy_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Store Boy"
            android:textSize="@dimen/_30sdp"
            android:background="@drawable/ev_editext__normal_bg">
        </workshop.lbit.qrcode.customfonts.MyTextView_Roboto_Medium>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_30sdp">

        <workshop.lbit.qrcode.customfonts.MyTextView_Roboto_Medium
            android:id="@+id/ll_gatepass_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Gate Pass"
            android:textSize="@dimen/_30sdp"
            android:background="@drawable/ev_editext__normal_bg">
        </workshop.lbit.qrcode.customfonts.MyTextView_Roboto_Medium>

    </LinearLayout>


</LinearLayout>